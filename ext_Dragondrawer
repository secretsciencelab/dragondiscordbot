import discord
import random
from discord.ext import commands

class Dragondrawer():
  def __init__(self, bot):
    self.bot = bot

  @commands.command()
  async def drawmedragon(self):
    await self.bot.say("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQ95sM0hinaHShVkpks90XSVBwnwlGn4txYeScUssBfGyAGo2njYw&s")

  @commands.command()
  async def age(self):
    await self.bot.say("eight?")

def setup(bot):
  bot.add_cog(Dragondrawer(bot))
